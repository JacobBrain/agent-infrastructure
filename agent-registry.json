  "agents": [
    {
      "id": "ada",
      "name": "Ada - Form Analyzer",
      "description": "Processes Tally form submissions for Workmanship ministry. Analyzes both 'Request for Help' and 'Apply to be a Helper' forms, then sends formatted email summaries to ministry coordinator.",
      "url": "https://workmanship-form-analyzer.jacob-788.workers.dev",
      "capabilities": [
        "form_analysis",
        "email_sending",
        "ministry_coordination",
        "request_triage"
      ],
      "triggers": [
        "webhook"
      ],
      "input_schema": {
        "formData": {
          "type": "object",
          "required": true,
          "description": "Tally webhook payload containing form submission data"
        },
        "formType": {
          "type": "string",
          "required": false,
          "description": "Optional: 'request' or 'helper'. Auto-detected if not provided."
        }
      },
      "output_schema": {
        "emailSent": {
          "type": "boolean",
          "description": "Whether email was successfully sent"
        },
        "formType": {
          "type": "string",
          "description": "Type of form processed: 'request' or 'helper'"
        },
        "summary": {
          "type": "string",
          "description": "Brief summary of the submission"
        }
      },
      "requires_env_vars": [
        "ANTHROPIC_API_KEY",
        "RESEND_API_KEY"
      ],
      "status": "production",
      "version": "1.0.0",
      "created_date": "2024-10-15",
      "last_modified": "2025-10-23"
    },
    {
      "id": "nova",
      "name": "Nova - Marketing Writer",
      "description": "Generates LinkedIn and Substack content in Jacob's voice. Pulls from Marketing Voice Examples database and AI Brain knowledge base to match tone, style, and brand guidelines.",
      "url": "https://marketing-writer.jacob-788.workers.dev",
      "capabilities": [
        "content_generation",
        "linkedin_posts",
        "substack_notes",
        "voice_matching",
        "brand_consistency"
      ],
      "triggers": [
        "mcp",
        "api"
      ],
      "input_schema": {
        "topic": {
          "type": "string",
          "required": true,
