        ],
        "pages": [
          {
            "id": "241abcd1b44580609e11d2e298cdf80a",
            "name": "AI Brain",
            "purpose": "General knowledge base with business context"
          },
          {
            "id": "241abcd1b445805399ccddc34a006653",
            "name": "LinkedIn Content Style Guide",
            "purpose": "Platform-specific formatting and tone guidelines"
          }
        ]
      },
