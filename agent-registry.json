          "type": "string",
          "required": false,
          "description": "Optional style notes or adjustments (e.g., 'make it shorter', 'more contrarian')"
        }
      },
      "output_schema": {
        "draft": {
          "type": "string",
          "description": "Generated content ready to post"
        },
        "voiceExamplesUsed": {
          "type": "number",
          "description": "Number of voice examples referenced"
        },
        "contextDocsUsed": {
          "type": "number",
          "description": "Number of Notion docs used for context"
        }
      },
      "requires_env_vars": [
        "ANTHROPIC_API_KEY",
        "NOTION_TOKEN",
        "NOTION_DATABASE_ID"
      ],
